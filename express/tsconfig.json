{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strictNullChecks": false
    },
    "include": ["src/**/*.ts", "test/**/*.ts"]
  }
  